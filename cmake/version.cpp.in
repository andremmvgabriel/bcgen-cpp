#include <bcgen/version.hpp>

const char* gabe::bcgen::get_version() {
    return "@PROJECT_VERSION@";
}

uint16_t gabe::bcgen::get_version_major() {
    return @PROJECT_VERSION_MAJOR@;
}

uint16_t gabe::bcgen::get_version_minor() {
    return @PROJECT_VERSION_MINOR@+0;
}

uint16_t gabe::bcgen::get_version_patch() {
    return @PROJECT_VERSION_PATCH@+0;
}

uint16_t gabe::bcgen::get_version_tweak() {
    return @PROJECT_VERSION_TWEAK@+0;
}
